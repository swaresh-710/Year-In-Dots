{
  "manifest_version": 3,
  "name": "YearInDots",
  "version": "1.0",
  "description": "A live wallpaper New Tab showing 365 days as dots.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}